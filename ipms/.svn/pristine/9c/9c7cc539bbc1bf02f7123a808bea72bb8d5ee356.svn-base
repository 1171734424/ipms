			} else {
				JSONUtil.responseJSON(response, new AjaxResult(1, "可以结算!"));
			}
		} else if ("4".equalsIgnoreCase(status)) {
			JSONUtil.responseJSON(response, new AjaxResult(0, "当前已结算!"));
		} else {
			JSONUtil.responseJSON(response, new AjaxResult(1, "可以结算!"));
		}
	}
	
