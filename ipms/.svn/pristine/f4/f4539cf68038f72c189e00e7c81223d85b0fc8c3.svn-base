package com.ideassoft.bean.templateMessage;

public class BeMemberMsg implements java.io.Serializable {

	private static final long serialVersionUID = -9024269171583404180L;
	// 您好，您已成为微信某某店会员
	private String first;
	private String firstColor;
	// 87457
	private String cardNumber;
	private String cardNumberColor;
	// 邹某某
	private String VIPName;
	private String VIPNameColor;
	// 1313213132
	private String VIPPhone;
	private String VIPPhoneColor;
	// 如有疑问，请咨询13912345678。
	private String remark;
	private String remarkColor;

	public String getFirst() {
		return first;
	}

	public void setFirst(String first) {
		this.first = first;
	}

	public String getFirstColor() {
		return firstColor;
	}

	public void setFirstColor(String firstColor) {
		this.firstColor = firstColor;
	}

	public String getCardNumber() {
		return cardNumber;
	}

	public void setCardNumber(String cardNumber) {
		this.cardNumber = cardNumber;
	}

	public String getCardNumberColor() {
		return cardNumberColor;
	}

	public void setCardNumberColor(String cardNumberColor) {
		this.cardNumberColor = cardNumberColor;
	}

	public String getVIPName() {
		return VIPName;
	}

	public void setVIPName(String vIPName) {
		VIPName = vIPName;
	}

	public String getVIPNameColor() {
		return VIPNameColor;
	}

	public void setVIPNameColor(String vIPNameColor) {
		VIPNameColor = vIPNameColor;
	}

	public String getVIPPhone() {
		return VIPPhone;
	}

	public void setVIPPhone(String vIPPhone) {
		VIPPhone = vIPPhone;
	}

	public String getVIPPhoneColor() {
		return VIPPhoneColor;
	}

	public void setVIPPhoneColor(String vIPPhoneColor) {
		VIPPhoneColor = vIPPhoneColor;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public String getRemarkColor() {
		return remarkColor;
	}

	public void setRemarkColor(String remarkColor) {
		this.remarkColor = remarkColor;
	}

}
