}

.btn_pie {
	height: 22px;
	padding-left: 20px;
	display: inline-block;
	margin-top: 3px;
	margin-left: 5px;
	background: url(../images/button/btn_pie.png) no-repeat;
	filter: "progid:DXImageTransform.Microsoft.AlphaImageLoader(sizingMethod='scale')";
	-moz-background-size: 100% 100%;
	background-size: 100% 100%;
}

.btn_column {
	height: 21px;
	padding-left: 20px;
	display: inline-block;
	margin-top: 5px;
	margin-left: 5px;
	background: url(../images/button/btn_column.png) no-repeat;
