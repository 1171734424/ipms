
body {
	margin: 0;
	padding: 0;
	overflow: hidden;
}
a{
	color:#333;
}
/*定义滚动条轨道 内阴影+圆角*/
/*定义滑块 内阴影+圆角*/  
::-webkit-scrollbar-thumb:horizontal {
	width: 5px;
	background-color: #ccc;
	-webkit-border-radius: 3px;
}

p {
	font-size: 15px;
	font-family: Microsoft YaHei;
    color: #535474;
}

.form {
	width: 100%;
	height: 100%;
}

.reportData {
	width: 100%;
	height: 94%;
	float: left;
}

.ul {
	margin: 0px;
	padding: 0px;
}
.ul li{
	-webkit-transition: all 0.12s ease, 0.12s ease;
	transition: all 0.12s ease, 0.12s ease;
}
.li {
	float: left;
	list-style-type: none;
}

