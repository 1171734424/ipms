.b_cellButton {
	height: 26px !important;
	width: 26px !important;
	cursor: pointer;
	background: transparent;
	background-color: black !important;
}

.b_multiSelect {
	background: url(../images/button/btn_unselect.png) no-repeat;
}

.fontspan {
    width: 93px;
    height: 30px;
    font-size: 14px;
    float: right;
    line-height: 19px;
    padding-top: 11px;
    color: #333;
}

.btn {
    width: 264px;
	height: 30px;
	float: right;
	margin-right: 5px;
}

.b_btn {
    width: 39px;
    height: 29px;
    line-height: 27px;
    cursor: pointer;
    display: inline-block;
    white-space: nowrap;
    border: solid 1px #33425B;
    margin-left: 5px;
    margin-top: 7px;
    text-align: center;
    border-radius: 3px;
    transition:all 0.3s ease-in-out;
}

.b_btn:hover {
	background:#535474;
}

.pageRegion {
	height: 42px;
    line-height: 36px;
    width: 100%;
    margin-top: 15px;
}
#pagerForm .pageRegion {
	position: absolute;
    bottom: 0;
}

.b_btn .b_first {
	padding: 4px 18px;
	margin-top: 4px;
	background: url(../../../images/button/first-page.png) center;
	background-repeat: no-repeat;
}
.b_btn .b_first:hover{
	background: url(../../../images/button/first-pagetwo.png) center;
	background-repeat: no-repeat;
}
.b_btn .b_preview {
	padding: 4px 18px;
	margin-top: 4px;
	background: url(../../../images/button/up-page.png) center;
	background-repeat: no-repeat;
}
.b_btn .b_preview:hover{
	background: url(../../../images/button/up-pagetwo.png) center;
	background-repeat: no-repeat;
}
.b_btn .b_looking {
	padding: 4px 18px;
	margin-top: 6px;
	background: url(../../../images/button/refresh-page.png) center;
	background-repeat: no-repeat;
}
.b_btn .b_looking:hover{
	background: url(../../../images/button/refresh-pagetwo.png) center;
	background-repeat: no-repeat;
}
.b_btn .b_next {
	padding: 4px 18px;
	margin-top: 4px;
	background: url(../../../images/button/next-page.png) center;
	background-repeat: no-repeat;
}
.b_btn .b_next:hover {
	background: url(../../../images/button/next-pagetwo.png) center;
	background-repeat: no-repeat;
}
.b_btn .b_last {
	padding: 4px 18px;
	margin-top: 4px;
	background: url(../../../images/button/last-page.png) center;	
	background-repeat: no-repeat;
}

.b_btn .b_last:hover {
	background: url(../../../images/button/last-pagetwo.png) center;
	background-repeat: no-repeat;
}

.btn input {
    width: 39px;
    height: 29px;
    border: solid 1px #33425B;
    text-align: center;
    margin-top: 7px;
    margin-left: 4px;
    border-radius: 3px;
}
.btn input:hover {
    background:#535474;
    color:#fff;
}
