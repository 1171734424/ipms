/*重写select*/
::-webkit-scrollbar{
  display:none;
}
.dropdown.open div{
	
}
.data-span{
	padding:13px 0;
    display: inline-block;
    width: 100%;
    text-align: left;
    color: #fff;
    font-size: 14px;
}
.branch-ul{
    width: 100%;
    background-color: rgba(0,0,0,0.5);
    margin-top: 3px;
    z-index: 999999;
    position: absolute;
    margin-left: -48px;
    border-radius: 4px;
    transition: all 0.3s ease-in-out;
    animation-duration: 0.3s;
    display: none;
    overflow-y: scroll;
}
.data-span.open ~ i{
	transition: all 0.3s ease-in-out;
    margin-top: -1px;
    display: inline-block;
    border-right: 2px solid;
    border-bottom: 2px solid;
    width: 15px;
    height: 15px;
    border-color: #fff;
    transform: rotate(-136deg);
}
.branch-ul ul{
	text-align:left;
}
.branch-ul ul li{
	height:34px;
	line-height:34px;
	width:100%;
	text-indent:48px;
	font-size:14px;
}
.branch-ul ul li:hover{
	background:rgba(0,0,0,0.6);
}
/*三角形制作*/
.dropdown .carat{
    transition: all 0.3s ease-in-out;
    margin-top: -8px;
    display: inline-block;
    border-right: 2px solid;
    border-bottom: 2px solid;
    width: 15px;
    height: 15px;
    transform: rotate(44deg);
    right: 12px;
    top: 50%;
    border-color: #fff;
    position: absolute;
}
.form-group .select_main{
	height:100%;
    width: 94%;
    border:none;
}
.form-group .select_main .select_content{
	padding: 15px 140px 12px 19px;
    line-height: 44px;
}
.form-group .select_arrow_after {
	border-top-color: transparent;
}
.form-group .select_list{
	position: absolute;
    left: 10px;
    top: 100%;
    width: inherit;
    border-top: none;
    background-color: rgba(255,255,255,0.8);
    overflow: hidden;
    display: none;
    z-index: 99;
    width: 282px;
    border-bottom-left-radius: 4px;
    border-bottom-right-radius: 4px;
}
.form-group .select_arrow, .form-group .select_arrow_after{
	top: 43%;
}
.form-group .select_content::after{
	box-shadow:none;
}
