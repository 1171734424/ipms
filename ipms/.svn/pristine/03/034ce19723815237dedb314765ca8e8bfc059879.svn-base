package com.ideassoft.crm.templateMessage;

public class ChargeMsg implements java.io.Serializable {

	private static final long serialVersionUID = 726511493299690442L;
	// 招呼：亲爱的会员，您好!您已成功充值。
	private String first;
	private String firstColor;
	// 账户类型：会员卡，或者会员等级，自由发挥
	private String accountType;
	private String accountTypeColor;
	// 会员卡号，或者会员号
	private String account;
	private String accountColor;
	// 充值金额
	private String amount;
	private String amountColor;
	// 充值状态：充值成功
	private String result;
	private String resultColor;
	// 备注
	private String remark;
	private String remarkColor;

	public String getFirst() {
		return first;
	}

	public void setFirst(String first) {
		this.first = first;
	}

	public String getFirstColor() {
		return firstColor;
	}

	public void setFirstColor(String firstColor) {
		this.firstColor = firstColor;
	}

	public String getAccountType() {
		return accountType;
	}

	public void setAccountType(String accountType) {
		this.accountType = accountType;
	}

	public String getAccountTypeColor() {
		return accountTypeColor;
	}

	public void setAccountTypeColor(String accountTypeColor) {
		this.accountTypeColor = accountTypeColor;
	}

	public String getAccount() {
		return account;
	}

	public void setAccount(String account) {
		this.account = account;
	}

	public String getAccountColor() {
		return accountColor;
	}

	public void setAccountColor(String accountColor) {
		this.accountColor = accountColor;
	}

	public String getAmount() {
		return amount;
	}

	public void setAmount(String amount) {
		this.amount = amount;
	}

	public String getAmountColor() {
		return amountColor;
	}

	public void setAmountColor(String amountColor) {
		this.amountColor = amountColor;
	}

	public String getResult() {
		return result;
	}

	public void setResult(String result) {
		this.result = result;
	}

	public String getResultColor() {
		return resultColor;
	}

	public void setResultColor(String resultColor) {
		this.resultColor = resultColor;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public String getRemarkColor() {
		return remarkColor;
	}

	public void setRemarkColor(String remarkColor) {
		this.remarkColor = remarkColor;
	}

}
