.class_main .class_detail{
	height: 84px;
    overflow-y: scroll;
    overflow-x: hidden;
    width: 99%;
}
.class_main{
	height:100%;
	background: palegoldenrod;
}
.class_main ul li{
	float: left;
    overflow-x: hidden;
    text-overflow: ellipsis;
}
.class_main ul li span{
	font-weight: bold;
    line-height: 32px;
    padding: 0 10px;
    display: inline-block;
    vertical-align: middle;
    white-space: nowrap;
    color: #0077b3;
    font-size: 0.85rem;
    cursor: pointer;
    text-decoration: underline;
}
.class_main .class_detail table{
	border:none;
    width: 95%;
    margin: 0 2%;
}
