    line-height: 27px;
    border: 1px solid #ccc;
    position: absolute;
    bottom:-85px;	
    right:11%;
}
.add_repair ul li:nth-child(8){
	margin:0 !important;
	float:none !important;
	position:absolute;
	bottom:23px;
}
.class_detail .confirm{
	position:absolute;
	right:2px;
	bottom:2px;
}

.rent_deadline table.myTable td{
