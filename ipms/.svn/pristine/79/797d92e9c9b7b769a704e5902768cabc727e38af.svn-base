/*容器*/
.check_wrap_margin{
	width: 100%;
    height: 100%;
    position: relative;
    background: #EFF3F5;
    box-shadow: 1px 1px 8px rgba(0, 0, 0, .3);
    border-radius: 4px;
    padding: 0 15px;
}
.check_color{
	
}
/*标题*/
.check_wrap_margin h2{
	width: 100%;
    font-size: 1.5rem;
    height: 3.2rem;
    color:#333333;
   	line-height: 3.2rem;
    padding-left: 10px;
}
.check_wrap_margin .imooc_cash{
	top: -1px;
    left: -2%;
    font-size: 38px;
}
/*头部tab选择区域*/
.top_check{
	height: 70px;
	background:#FFFFFF;
}
.left_check{
	width: 89.11%;
	margin-left: 68px;	
	border-bottom:1px solid #DDDDDD;
}
.left_check .cn{
	font-size:1.2rem;
}
.left_check ul li{
    float: left;
    width: 14.66%;
    font-size: 0.95rem;
    padding: 10px;
    margin-top: 24px;
    text-align: center;
    background:#F0F0F0;
}
.left_check ul li span{
	cursor:pointer;
}
.rno{
	font-size:2rem;
}
.cm{
	margin-left: 25px;
}
.right_check{
	width: 28%;
    margin-top: 18px;
    margin-right: 105px;
   	border-bottom:1px solid #DDDDDD;
}

/*tab1内容区域*/
.content_color{
	height:639px;
	background:#FFFFFF;
}
/*备用金柜select*/
.cabinet_cash{
	width:100%;
}
/*textarea*/
.card{

	
}
.card .label_name{
	margin-top:9px;
	margin-left: 5px;
}
.check_one{
	width:100%;
	padding-left: 20px;
}
.check_one ul li{
	float: left;
    margin: 14px 11px 14px 6px;
    line-height: 38px;
    margin-right: 0px;
}
.check_one .label_name,.check_two .label_name{
    float: left;
    width: 111px;
    text-align: right;
    font-weight: normal;	
    font-size: 0.85rem;
}
input,select,textarea{
    border-radius: 3px;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    color: #323232;
    background-color: #fff;
    border: 1px solid #DDDDDD;
    line-height: 1.2;
    font-size: 0.85rem;
    -webkit-box-shadow: none!important;
    box-shadow: none!important;
    -webkit-transition-duration: .1s;
    transition-duration: .1s;
}
.content_color select,.content_color textarea{
	border:1px solid #DDDDDD;
}
.content_color  textarea{
   width: 62.56%;
    padding-left: 5px;
    margin: 10px 0 0 10px;
}
.check_one .check,.check_two .check{
    padding: 8px 5px;
	width: 227px;
    margin-left:10px;
}
.check_two .check_ul{
	margin:0 20px;
	border-bottom: 1px solid #CCCCCC;
}
.check_two ul li.check_name{
    border: 1px solid #CCCCCC;
    width: 85px;
    text-align: center;
}
.button_margin{
	float:right;
    height: 34px;
    line-height: 31px;
    text-align: center;
    width: 101px;
    margin-right:10px;
    margin-top:10px;
    border: 1px solid #CCCCCC;
    cursor:pointer;
}
.left_check .tab_select{
	background-color:#FFFFFF;
}
.left_div{
	margin: 31px 0 0 0;
    color: red;
    font-size: 20px;
}

.card ul li{
	margin: 15px 0px 13px 0.6px;
    width: 32.3%;
	}


#remark{
	height:30%;
	width: 81.8%;
	}

.cashdata_mss {
    width: 100%;
    background: #fff;
    margin-top: -7.4%;
