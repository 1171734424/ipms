function refreshprice(){
		var price = $("#price").val();
		var houseId = $("#houseName_CUSTOM_VALUE").val();
		var startTime = $("#arrivedate").val();
		var endTime = $("#leavedate").val();
		if(new Date(startTime).getTime() >= new Date(endTime).getTime()){
			showMsg("请选择正确的日期");
			return false;
		}
		if(price != ""){
			$.ajax({
		        url: base_path + "/houseChoose.do",
				 type: "post",
				 dataType : "json",
				 data : {"houseId":houseId,
						 "startTime":startTime,
						 "endTime":endTime},
				 success: function(json) {
						if(json.result =="0" || json.result == "2"){
							$("#houseName").val("");
							$("#houseName_CUSTOM_VALUE").val("");
							$("#price").val("");
							showMsg(json.message);
							return;
						}
						if(json.result =="1"){//民宿可订
							$("#price").val(json.message);
						}
				 },
				 error: function(json) {
					 showMsg(json.message);
				 }
			});
		}
	
}
