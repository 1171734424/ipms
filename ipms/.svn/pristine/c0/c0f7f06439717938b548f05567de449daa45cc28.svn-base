	public void setTodayWater(String todayWater) {
		this.todayWater = todayWater;
	}
	public String getTodayWaterCost() {
		return todayWaterCost;
	}
	public void setTodayWaterCost(String todayWaterCost) {
		this.todayWaterCost = todayWaterCost;
	}
