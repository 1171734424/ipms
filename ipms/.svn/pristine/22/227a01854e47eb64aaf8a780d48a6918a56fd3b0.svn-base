 .accordion {	
    width: 100%;
    height: 712px;
    max-width: 191px;
    background: #FFF;
 }
.accordion .link {
	cursor: pointer;
	display: block;
    padding: 10px 0px 10px 42px;
    color: #FFFFFF;
    background: #4A5064;
	font-size: 14px;
	font-weight: 700;
	border-bottom: 1px solid #CCC;
	position: relative;
	-webkit-transition: all 0.4s ease;
	-o-transition: all 0.4s ease;
	transition: all 0.4s ease;
	position:relative;
}
.accordion .link:after{
 	content: '';
    display: block;
    width: 0;
    height: 0;
    border-width: 20px 0 20px 8px;
    border-style: solid;
    border-color: transparent transparent transparent #4A5064;
    position: absolute;
    top: -1px;
    right: -8px;
    z-index: 999;
}
.accordion li:last-child .link {
	border-bottom: 0;
}

.accordion li i.fa-angle-down{
	position: absolute;
	top: 9px;
    right: 12px;
    font-size: 22px;
	color: #FFFFFF;
	-webkit-transition: all 0.4s ease;
	-o-transition: all 0.4s ease;
	transition: all 0.4s ease;
}
.accordion li.open .link {
	color: #FFFFFF;
}
.accordion li.open i {
	color: #333;
}
.accordion li.open i.fa-chevron-down {
	-webkit-transform: rotate(180deg);
	-ms-transform: rotate(180deg);
	-o-transform: rotate(180deg);
	transform: rotate(180deg);
}
/**
 * Submenu
 -----------------------------*/
 .submenu {
 	display: block;
 	background: #FFFFFF;
 	color:#333333;
 	font-size: 14px;
 	position:relative;
 }
.submenu li {
 	border-bottom: 1px solid #f0f0f0;
    position: relative;
 }
.submenu a.active{
	font-weight: bold;
}
.submenu a {
 	display: block;
 	text-decoration: none;
 	color: #333;
 	padding: 10px;
 	padding-left: 42px;
 	-webkit-transition: all 0.25s ease;
 	-o-transition: all 0.25s ease;
 	transition: all 0.25s ease;
 	cursor: pointer;
 }
.submenu a:hover {
 	background: #EAEAEA;
 	color: #333;
 }
.submenu li .fa{
	color: #4a5064;
    position: absolute;
    top: 12px;
    left: 15px;
}
