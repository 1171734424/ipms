@keyframes scope_ccw {
	from {
		transform: rotate(0deg)
	}
	to {
		transform: rotate(-360deg)
	}
}
@keyframes scope_ccw {
	from {
		transform: rotate(-360deg)
	}
	to {
		transform: rotate(0deg)
	}
}
