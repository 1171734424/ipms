/*重写班次选择样式*/
.loginReg-viewbox input.labelauty + label{
	padding:0;
	text-align: center;
	box-shadow: 0px 0px 0px 5px rgba(255,255,255,0.2);
}

.loginReg-viewbox input.labelauty + label > span.labelauty-unchecked, input.labelauty + label > span.labelauty-checkedP{
	line-height: 80px;
}

.loginReg-viewbox input.labelauty:checked + label > span.labelauty-checked{
	line-height: 80px;
}

.loginReg-viewbox input.labelauty:checked + label{
	background-color: #ff6231;
}

.loginReg-viewbox  input.labelauty:not(:checked):not([disabled]) + label:hover,.loginReg-viewbox input.labelauty:checked:not([disabled]) + label:hover
{
	background-color: #ff6231;
	color:#fff;
}












