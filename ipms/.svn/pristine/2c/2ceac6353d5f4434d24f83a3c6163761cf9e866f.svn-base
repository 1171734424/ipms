@charset "utf-8";
p
{
	margin: 0;
	padding: 0;
}
._filelist
{
	list-style: none;
	margin: 0;
	padding: 0;
}
._filelist:after
{
	clear: both;
	content: '';
	display: block;
	height: 0;
	overflow: hidden;
	width: 0;
}
._filelist li
{
	background: url(../../../images/picadd/bg.png) repeat;
	display: inline;
	float: left;
	font-size: 12px;
	height: 160px;
	margin: 0 8px 20px 0;
	overflow: hidden;
	position: relative;
	text-align: center;
	width: 180px;
}
._filelist li p.log
{
	position: relative;
	top: -45px;
}
._filelist li p.title
{
	left: 0;
	overflow: hidden;
	position: absolute;
	text-align: left;
	text-indent: 5px;
	text-overflow: ellipsis;
	top: 0;
	top: 5px;
	visibility: hidden;
	white-space: nowrap;
	width: 100%;
}
._filelist li p.progress
{
	background: 0;
	border-radius: 0;
	bottom: 0;
	height: 8px;
	left: 0;
	margin: 0;
	overflow: hidden;
	position: absolute;
	webkit-box-shadow: 0;
	width: 100%;
	z-index: 50;
}
._filelist li p.progress span
{
	animation: progressmove 2s linear infinite;
	background: #1483d8 url(.../../../images/picadd/progress.png) repeat-x;
	display: none;
	height: 100%;
	moz-animation: progressmove 2s linear infinite;
	moz-transition: width 200ms linear;
	ms-animation: progressmove 2s linear infinite;
	ms-transition: width 200ms linear;
	o-animation: progressmove 2s linear infinite;
	o-transition: width 200ms linear;
	overflow: hidden;
	transition: width 200ms linear;
	webit-transition: width 200ms linear;
	webkit-animation: progressmove 2s linear infinite;
	webkit-transform: translateZ(0);
	width: 0;
}
@-webkit-keyframes progressmove
{
	0%
	{
		background-position: 0 0;
	}
	100%
	{
		background-position: 17px 0;
	}
}
@-moz-keyframes progressmove
{
	0%
	{
		background-position: 0 0;
	}
	100%
	{
		background-position: 17px 0;
	}
}
@keyframes progressmove
{
	0%
	{
		background-position: 0 0;
	}
	100%
	{
		background-position: 17px 0;
	}
}
._filelist li p.imgWrap
{
	height: 100%;
	moz-transform-origin: 50% 50%;
	moz-transition: 200ms ease-out;
	ms-transform-origin: 50% 50%;
	ms-transition: 200ms ease-out;
	o-transform-origin: 50% 50%;
	o-transition: 200ms ease-out;
	overflow: hidden;
	transform-origin: 50% 50%;
	transition: 200ms ease-out;
	vertical-align: middle;
	webit-transition: 200ms ease-out;
	webkit-transform-origin: 50% 50%;
	width: 100%;
	z-index: 2;
}
._filelist li img
{
	left: 50%;
	position: absolute;
	top: 50%;
}
._filelist li p.error
{
	background: #f43838;
	bottom: 0;
	color: #fff;
	height: 28px;
	left: 0;
	line-height: 28px;
	position: absolute;
	width: 100%;
	z-index: 100;
}
._filelist li p .retry-this,._filelist li p .del-this
{
	color: #d0d0d0;
	text-decoration: none;
}
._filelist li p .retry-this:hover,._filelist li p .del-this:hover
{
	color: #fff;
	text-decoration: none;
}
._filelist li .success
{
	background: url(../../../images/picadd/success.png) no-repeat right bottom;
	bottom: 0;
	display: block;
	height: 40px;
	left: 0;
	position: absolute;
	width: 100%;
	z-index: 200;
}
._filelist div.file-panel
{
	background: rgba(0,0,0,0.5);
	background-color: #000\9;
	filter: alpha(opacity=50)\9;
	filter: progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#80000000',endColorstr='#80000000')\0;
	height: 0;
	left: 0;
	overflow: hidden;
	position: absolute;
	top: 0;
	width: 100%;
	z-index: 300;
}
._filelist div.file-panel span
{
	background: url(../../../images/picadd/webuploader-icons.png) no-repeat;
	cursor: pointer;
	display: inline;
	float: right;
	height: 24px;
	margin: 5px 1px 1px;
	overflow: hidden;
	text-indent: -9999px;
	width: 24px;
}
._filelist div.file-panel span.rotateLeft
{
	background-position: 0 -24px;
}
._filelist div.file-panel span.rotateLeft:hover
{
	background-position: 0 0;
}
._filelist div.file-panel span.rotateRight
{
	background-position: -24px -24px;
}
._filelist div.file-panel span.rotateRight:hover
{
	background-position: -24px 0;
}
._filelist div.file-panel span.cancel
{
	background-position: -48px -24px;
}
._filelist div.file-panel span.cancel:hover
{
	background-position: -48px 0;
}
.webuploader-container
{
	position: relative;
}
.webuploader-element-invisible
{
	clip: rect(1px 1px 1px 1px);
	clip: rect(1px,1px,1px,1px);
	position: absolute!important;
}
.webuploader-pick
{
	background: #00b7ee;
	border-radius: 3px;
	color: #fff;
	cursor: pointer;
	display: inline-block;
	height: 100%;
	overflow: hidden;
	position: relative;
	text-align: center;
	width: 100%;
}
.webuploader-pick-hover
{
	background: #00a2d4;
}
.webuploader-pick-disable
{
	opacity: .6;
	pointer-events: none;
}