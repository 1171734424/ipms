package com.ideassoft.crm.templateMessage;

public class ServiceMsg implements java.io.Serializable {

	private static final long serialVersionUID = 7725719430199172806L;
	private String first;
	private String firstColor;
	// 酒店房号
	private String roomId;
	private String roomIdColor;
	// 服务类型
	private String ServiceType;
	private String ServiceTypeColor;
	// 服务内容
	private String ServiceContent;
	private String ServiceContentColor;
	private String remark;
	private String reamrkColor;

	public String getFirst() {
		return first;
	}

	public void setFirst(String first) {
		this.first = first;
	}

	public String getFirstColor() {
		return firstColor;
	}

	public void setFirstColor(String firstColor) {
		this.firstColor = firstColor;
	}

	public String getRoomId() {
		return roomId;
	}

	public void setRoomId(String roomId) {
		this.roomId = roomId;
	}

	public String getRoomIdColor() {
		return roomIdColor;
	}

	public void setRoomIdColor(String roomIdColor) {
		this.roomIdColor = roomIdColor;
	}

	public String getServiceType() {
		return ServiceType;
	}

	public void setServiceType(String serviceType) {
		ServiceType = serviceType;
	}

	public String getServiceTypeColor() {
		return ServiceTypeColor;
	}

	public void setServiceTypeColor(String serviceTypeColor) {
		ServiceTypeColor = serviceTypeColor;
	}

	public String getServiceContent() {
		return ServiceContent;
	}

	public void setServiceContent(String serviceContent) {
		ServiceContent = serviceContent;
	}

	public String getServiceContentColor() {
		return ServiceContentColor;
	}

	public void setServiceContentColor(String serviceContentColor) {
		ServiceContentColor = serviceContentColor;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public String getReamrkColor() {
		return reamrkColor;
	}

	public void setReamrkColor(String reamrkColor) {
		this.reamrkColor = reamrkColor;
	}

}
